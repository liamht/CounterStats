<UserControl x:Class="CounterStats.UI.Views.Elements.TwoColumnRow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CounterStats.UI.Views.Elements"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition  Width="1*"></ColumnDefinition>
            <ColumnDefinition  Width="1*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <ContentControl x:Name="ColumnOneContent" Grid.Column="0"></ContentControl>
        <ContentControl x:Name="ColumnTwoContent" Grid.Column="1"></ContentControl>
    </Grid>
</UserControl>
